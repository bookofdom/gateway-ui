{{#ap-list-nav-vertical}}
  {{#each store_collections as |storeCollection|}}
    {{#unless storeCollection.isNew}}
      {{ap-list-link
        path='store-collection'
        arg1=storeCollection
        t=storeCollection.name
        classNames='text-capitalize'}}
    {{/unless}}
  {{/each}}
  {{ap-list-link
    path='store-collections.new'
    t='actions.new'
    icon='the-essentials-010'}}
{{/ap-list-nav-vertical}}
