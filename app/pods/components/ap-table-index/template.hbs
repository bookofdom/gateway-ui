{{#bs-table headings=headings}}
  {{#each models as |model|}}
    {{#unless model.isNew}}
      {{ap-table-index-row
        model=model
        fields=fields
        path=path
        edit-path=edit-path
        show-delete=show-delete
        custom-primary-action=custom-primary-action
        custom-primary-icon=custom-primary-icon
        custom-primary-t=custom-primary-t}}
    {{/unless}}
  {{/each}}
{{/bs-table}}
