{{!-- begin placeholders to vertically offset new panel --}}
{{#ap-list-nav-vertical-circle-icon class='invisible hidden-xs'}}
  {{ap-list-link
      class='ap-unsortable'
      icon='the-essentials-129'}}

  {{#each model as |component|}}
    {{#unless component.isNew}}
      {{ap-list-link
        class='ap-unsortable'
        icon='the-essentials-129'}}
    {{/unless}}
  {{/each}}
{{/ap-list-nav-vertical-circle-icon}}
{{!-- end placeholders --}}

{{#bs-panel t='actions.select-add-component-type'}}
  {{#ap-list-nav-circle-icon}}
    {{ap-list-link
      t='types.proxy-endpoint-component.single'
      icon='the-essentials-072'
      path=path
      arg1='single'}}
    {{ap-list-link
      t='types.proxy-endpoint-component.multi'
      icon='multiproxy'
      path=path
      arg1='multi'}}
    {{ap-list-link
      t='types.proxy-endpoint-component.js'
      icon='file-type-content-055'
      path=path
      arg1='js'}}
    {{ap-list-link
      t='types.proxy-endpoint-component.shared'
      icon='the-essentials-072'
      path=path
      arg1='shared'}}
  {{/ap-list-nav-circle-icon}}
{{/bs-panel}}
