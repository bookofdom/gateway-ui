{{#ap-page t=tertiary-layout-header help-url=(docsUrl 'proxy-endpoints')}}
  <div class="ap-app-tertiary">
    <div class="ap-app-tertiary-sidebar">

      <div class="hidden-xs">
        {{#ap-list-nav-vertical-circle-icon}}
          {{partial 'layouts/tertiary/nav-items'}}
        {{/ap-list-nav-vertical-circle-icon}}
        {{#if view.http}}
          {{#ap-list-nav-vertical-circle-icon}}
            {{partial 'layouts/tertiary/nav-items-secondary'}}
          {{/ap-list-nav-vertical-circle-icon}}
        {{/if}}
      </div><!-- /.hidden-xs -->

      <div class="visible-xs">
        <div class="dropdown">
          <a class="dropdown-toggle" href="#" data-toggle="dropdown">
            {{t 'actions.navigate'}}
            <span class="caret"></span>
          </a><!-- /.dropdown-toggle -->
          <ul class="dropdown-menu">
            {{partial 'layouts/tertiary/nav-items'}}
            {{#if view.http}}
              <li class="divider"></li>
              {{partial 'layouts/tertiary/nav-items-secondary'}}
            {{/if}}
          </ul><!-- /.dropdown-menu -->
        </div><!-- /.dropdown -->
        <br>
      </div><!-- /.visible-xs -->

      {{outlet 'sidebar'}}

    </div><!-- /.ap-app-tertiary-sidebar -->
    <div class="ap-app-tertiary-content">
      {{outlet}}
    </div><!-- /.ap-app-tertiary-content -->
  </div><!-- /.ap-app-tertiary -->
{{/ap-page}}
