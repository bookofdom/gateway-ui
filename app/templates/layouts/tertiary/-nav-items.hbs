{{ap-list-link
    class='ap-inverse ap-unsortable'
    path='proxy-endpoint.edit'
    icon='the-essentials-129'
    t=name}}

{{#each components}}
  {{#unless isNew}}
    {{#if single}}
      {{ap-list-link
        path='proxy-endpoint-component.edit'
        action='proxy-endpoint-component-edit'
        arg1=this
        icon='the-essentials-072'
        t=name
        index=id}}
    {{/if}}
    {{#if multi}}
      {{ap-list-link
        path='proxy-endpoint-component.edit'
        action='proxy-endpoint-component-edit'
        arg1=this
        icon-type='gateway'
        icon='multiproxy'
        t=name
        index=id}}
    {{/if}}
    {{#if js}}
      {{ap-list-link
        path='proxy-endpoint-component.edit'
        action='proxy-endpoint-component-edit'
        arg1=this
        icon='file-type-content-055'
        t=name
        index=id}}
    {{/if}}
  {{/unless}}
{{/each}}

{{ap-list-link
    class='ap-inverse ap-unsortable'
    path='proxy-endpoint-components.new'
    icon='the-essentials-104'
    t='actions.new'}}

{{ap-list-link
    class='ap-inverse ap-unsortable'
    path='proxy-endpoint-tests'
    icon='health-care-medical-037'
    t='resources.proxy-endpoint-test_plural'}}
