{{#ap-model-form
    model=model
    fields=fields
    show-delete=true}}
  <br>
  {{#each before as |transform|}}
    {{render 'proxy-endpoint-component-transformation.inline-before-form' transform}}
  {{/each}}

  {{render 'proxy-endpoint-component-call.inline-form' model.call}}

  {{#each after as |transform|}}
    {{render 'proxy-endpoint-component-transformation.inline-after-form' transform}}
  {{/each}}
{{/ap-model-form}}
