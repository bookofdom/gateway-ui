{{!-- begin placeholders to vertically offset new panel --}}
{{#ap-list-nav-vertical-circle-icon class='invisible'}}
  {{ap-list-link
      class='ap-unsortable'
      path='proxy-endpoint.edit'
      icon='the-essentials-129'
      t=model.name}}

  {{#each component in model.components}}
    {{#unless component.isNew}}
      {{#if component.single}}
        {{ap-list-link
          path='proxy-endpoint-component.edit'
          action='proxy-endpoint-component-edit'
          arg1=component
          icon='the-essentials-072'
          t=component.name
          index=id}}
      {{/if}}
      {{#if component.multi}}
        {{ap-list-link
          path='proxy-endpoint-component.edit'
          action='proxy-endpoint-component-edit'
          arg1=component
          icon='multiproxy'
          icon-type='gateway'
          t=component.name
          index=id}}
      {{/if}}
      {{#if component.js}}
        {{ap-list-link
          path='proxy-endpoint-component.edit'
          action='proxy-endpoint-component-edit'
          arg1=component
          icon='file-type-content-055'
          t=component.name
          index=id}}
      {{/if}}
    {{/unless}}
  {{/each}}
{{/ap-list-nav-vertical-circle-icon}}
{{!-- end placeholders --}}


{{#bs-panel t='actions.select-add-component-type'}}
  {{#ap-list-nav-circle-icon}}
    {{ap-list-link
      t='proxy-endpoint-component-types.single-proxy'
      icon='the-essentials-072'
      path='proxy-endpoint-components.new.type'
      arg1='single'}}
    {{ap-list-link
      t='proxy-endpoint-component-types.multi-proxy'
      icon='multiproxy'
      icon-type='gateway'
      path='proxy-endpoint-components.new.type'
      arg1='multi'}}
    {{ap-list-link
      t='proxy-endpoint-component-types.javascript-logic'
      icon='file-type-content-055'
      path='proxy-endpoint-components.new.type'
      arg1='js'}}
  {{/ap-list-nav-circle-icon}}
{{/bs-panel}}
