{{#each field in fieldsList}}
  {{ap-table-index-cell
    model=model
    field=field
    path=path}}
{{/each}}

<td>
  {{#ap-list-nav-simple-icon right=true}}
    {{#if custom-primary-action}}
      {{ap-list-link
        t=custom-primary-t
        icon=custom-primary-icon
        action='custom-primary'}}
    {{/if}}
    {{#if edit-path}}
      {{ap-list-link
        t='actions.edit'
        icon='the-essentials-027'
        path=edit-path
        arg1=model}}
    {{/if}}
    {{#if show-delete}}
      {{ap-list-link
        t='actions.delete'
        icon='the-essentials-011'
        action='delete'}}
    {{/if}}
  {{/ap-list-nav-simple-icon}}
</td>
