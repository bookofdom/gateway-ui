{{#bs-table}}
  {{#each models as |model|}}
    {{#ap-table-model-row
      model=model
      edit-path=edit-path
      custom-primary-action=custom-primary-action
      custom-primary-icon=custom-primary-icon
      custom-primary-t=custom-primary-t}}
      {{#if partial-path}}
        {{partial partial-path}}
      {{/if}}
      {{#if component-name}}
        {{component component-name model=model indexModel=indexModel option-groups=option-groups}}
      {{/if}}
    {{/ap-table-model-row}}
  {{/each}}
  {{yield}}
{{/bs-table}}
