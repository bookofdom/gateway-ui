<br>
{{#each model.before as |transform|}}
  {{#unless transform.isDeleted}}
    {{forms/shared-component-transformation-inline-form before=true model=transform option-groups=transformation-option-groups}}
  {{/unless}}
{{/each}}

{{#each model.calls as |call|}}
  {{forms/shared-component-call-inline-form model=call indexModel=callsIndexModel option-groups=calls-option-groups}}
{{/each}}

{{#each model.after as |transform|}}
  {{#unless transform.isDeleted}}
    {{forms/shared-component-transformation-inline-form after=true model=transform option-groups=transformation-option-groups}}
  {{/unless}}
{{/each}}
