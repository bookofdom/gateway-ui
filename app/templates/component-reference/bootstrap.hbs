{{#bs-container}}
  
  {{#bs-row}}
    {{#bs-col sm=12}}
      <div class="page-header">
        {{bs-h1 t='Bootstrap' secondary='Ember Component Reference'}}
      </div>
    {{/bs-col}}
  {{/bs-row}}
  
  {{#bs-row}}
    {{#bs-col sm=12}}
      {{bs-h2 t='Default Options'}}
    {{/bs-col}}
  {{/bs-row}}
  
  {{#bs-row}}
    {{#bs-col sm=12}}
      <p>All components have the following options.</p>
    {{/bs-col}}
    {{#bs-col sm=6}}
      {{bs-h3 t='Misc'}}
      {{#bs-table headings='Option,Description' bordered=true}}
        <tr><th><code>active</code></th><td>Optional boolean.  Adds an <code>active</code> class.</td></tr>
        <tr><th><code>disabled</code></th><td>Optional boolean.  Adds a <code>disabled</code> class.</td></tr>
        <tr><th><code>clearfix</code></th><td>Optional boolean.  Adds a <code>clearfix</code> class.</td></tr>
        <tr><th><code>left</code></th><td>Optional boolean.  Adds a <code>pull-left</code> class.</td></tr>
        <tr><th><code>right</code></th><td>Optional boolean.  Adds a <code>pull-right</code> class.</td></tr>
      {{/bs-table}}
    {{/bs-col}}
    {{#bs-col sm=6}}
      {{bs-h3 t='Visibility'}}
      {{#bs-table headings='Option,Description' bordered=true}}
        <tr><th><code>visible-*-block</code></th><td>Optional boolean.  <code>*</code> is one of <code>xs</code>, <code>sm</code>, <code>md</code>, <code>lg</code></td></tr>
        <tr><th><code>visible-*-inline</code></th><td>Optional boolean.  <code>*</code> is one of <code>xs</code>, <code>sm</code>, <code>md</code>, <code>lg</code></td></tr>
        <tr><th><code>visible-*-inline-block</code></th><td>Optional boolean.  <code>*</code> is one of <code>xs</code>, <code>sm</code>, <code>md</code>, <code>lg</code></td></tr>
        <tr><th><code>hidden-*</code></th><td>Optional boolean.  <code>*</code> is one of <code>xs</code>, <code>sm</code>, <code>md</code>, <code>lg</code></td></tr>
      {{/bs-table}}
    {{/bs-col}}
    {{bs-breakpoint}}
    {{#bs-col sm=6}}
      {{bs-h3 t='Text Alignment' secondary='Only one allowed'}}
      {{#bs-table headings='Option,Description' bordered=true}}
        <tr><th><code>text-left</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>text-right</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>text-center</code></th><td>Optional boolean.</td></tr>
      {{/bs-table}}
    {{/bs-col}}
    {{#bs-col sm=6}}
      {{bs-h3 t='Text Transform' secondary='Only one allowed'}}
      {{#bs-table headings='Option,Description' bordered=true}}
        <tr><th><code>uppercase</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>lowercase</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>capitalize</code></th><td>Optional boolean.</td></tr>
      {{/bs-table}}
    {{/bs-col}}
    {{bs-breakpoint}}
    {{#bs-col sm=6}}
      {{bs-h3 t='Text Context Color' secondary='Only one allowed'}}
      
      {{#bs-table headings='Option,Description' bordered=true}}
        <tr><th><code>text-muted</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>text-primary</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>text-success</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>text-info</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>text-warning</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>text-danger</code></th><td>Optional boolean.</td></tr>
      {{/bs-table}}
    {{/bs-col}}
    {{#bs-col sm=6}}
      {{bs-h3 t='Background Context Color' secondary='Only one allowed'}}
      {{#bs-table headings='Option,Description' bordered=true}}
