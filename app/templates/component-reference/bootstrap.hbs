        <tr><th><code>bg-primary</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>bg-success</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>bg-info</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>bg-warning</code></th><td>Optional boolean.</td></tr>
        <tr><th><code>bg-danger</code></th><td>Optional boolean.</td></tr>
      {{/bs-table}}
    {{/bs-col}}
  {{/bs-row}}
  
